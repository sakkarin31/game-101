<Background>:
    id: background
    canvas.before:
        Rectangle:
            size: self.size
            pos: self.pos
            source: "background.png"
<Character>:
    id: character
    source: "character.png"
    size_hint: None, None
    size: 400,300
    pos: self.pos
<Label>:
    id: score
    size_hint_y: None
    height: 96
    text: "0"
    font_size: 40